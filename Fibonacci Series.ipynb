    "#Question:1 Fibonacci Series\n",
    "n=int(input(\"Enter the number upto which series to be print: \"))\n",
    "a=0\n",
    "b=1\n",
    "if n<=0:\n",
    "    print(\"Please enter the number greater than zero\")\n",
    "else:\n",
    "    print(a)\n",
    "    print(b)\n",
    "    for i in range(2, n):\n",
    "        c=a+b\n",
    "        a=b\n",
    "        b=c\n",
    "        print(c)"
